        if (age >= 0 && age <= 1) {
            return 'Infant';
        } else if (age >= 2 && age <= 14) {
            return 'Child';
        } else if (age >= 15 && age <= 21) {
            return 'Youth';
        } else if (age >= 22) {
            return 'Adult';
        } else {
            return ''; // Return empty string if age is negative or invalid
        }