public String whichTwo(Integer a, Integer b, Integer c) {
      // Check if any two numbers add up to the third
        if (a + b == c) {
            return 'c';
        } else if (a + c == b) {
            return 'b';
        } else if (b + c == a) {
            return 'a';
        } else {
            return '';  // No two numbers add up to the third
        }
    
}